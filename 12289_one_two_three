#include <bits/stdc++.h>
using namespace std;

int main(){
	int n;
	string word;
	
	cin>>n;
	for(int i=0; i<n; i++){
		cin>>word;
		
		int p = word.length();
		if(p==5) cout<<3<<endl;
    
		else{
			if(word[0]=='o' && word[1]=='n')
     		cout<<1<<endl;
        
     		    else if(word[1]=='n'&&word[2]=='e')
     			        cout<<1<<endl;
                  
			      else if(word[2]=='e'&&word[0]=='o')
		 		         cout<<1<<endl;
                 
		 		    else{
		 			       cout<<2<<endl;
				    }
		}
	}
}
